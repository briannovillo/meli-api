{
  "name": "ts-api-boilerplate",
  "version": "1.0.0",
  "description": "A powerfull boilerplate for Typescript API's",
  "main": "index.js",
  "scripts": {
    "dev": "tsc-watch --onSuccess \"sls offline start\"",
    "deploy:prd": "sls deploy --stage='dev' --aws-profile=brian",
    "apidoc": "npx apidoc -i ./src/routes/ -o doc/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/briannovillo/ts-api-boilerplate.git"
  },
  "keywords": [
    "ts",
    "api",
    "boilerplate"
  ],
  "author": "Brian Novillo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/briannovillo/ts-api-boilerplate/issues"
  },
  "homepage": "https://github.com/briannovillo/ts-api-boilerplate#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "serverless-http": "^2.3.0"
  },
  "devDependencies": {
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.11",
    "@types/node-fetch": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "apidoc": "^0.17.7",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "prettier": "^1.18.2",
    "serverless": "^1.53.0",
    "serverless-offline": "^5.12.0",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.6.3"
  },
  "apidoc": {
    "name": "MELI API",
    "title": "A powerfull wrapper for MELI API",
    "url": "http://briannovillo.github.io/meli_api",
